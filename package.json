{
	"name": "atomize-d",
	"main": "./dist/atomize-d",
	"version": "0.1.0",
	"description": "Adds D language support for Atom",
	"keywords": ["d", "dcd", "dfmt", "dscanner", "workspace-d", "ide", "autocomplete", "lint", "format"],
	"repository": "https://github.com/Vild/atomize-d",
	"license": "MIT",
	"engines": {
		"atom": ">=1.0.0 <2.0.0"
	},
	"scripts": {
		"test": "atom --test spec",
		"clean": "shx rm -rf dist",
		"tsc": "tsc -p src/tsconfig.json || echo done",
		"tsc.dev": "tsc -p src/tsconfig.json --watch",
		"build": "npm run tsc",
		"dev": "npm run tsc.dev",
		"build-commit": "build-commit -o dist",
		"prepare": "npm run build"
	},
	"providedServices": {
		"autocomplete.provider": {
			"versions": {
				"2.0.0": "provideAutocomplete"
			}
		},
		"linter": {
			"versions": {
				"1.0.0": "provideLinter"
			}
		}
	},
	"dependencies": {},
	"devDependencies": {
		"@types/atom": "1.40.7",
		"@types/node": "^14.14.22",
		"typescript": "^4.1.3",
		"shx": "0.3.3",
		"cross-env": "7.0.3",
		"build-commit": "0.1.4"
	}
}
